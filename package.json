{
  "name": "leetcode-trip",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:YanceyOfficial/leetcode-trip.git",
  "author": "YanceyOffical <yanceyofficial@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "precommit": "yarn test"
  },
  "devDependencies": {
    "husky": "^3.0.8",
    "jest": "^24.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
